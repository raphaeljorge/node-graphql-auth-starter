{
  "name": "node-graphql-auth-starter",
  "version": "1.0.0",
  "description": "Advanced Graphql boilerplate with authentication",
  "main": "index.js",
  "scripts": {
    "setup": "npm run setup:install && npm run setup:db",
    "setup:install": "npm install",
    "setup:db": "npm run db:migrate && npm run db:seed",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "start": "nodemon src/index.js --exec babel-node",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raphaeljorge/node-graphql-auth-starter.git"
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "graphql",
    "authentication"
  ],
  "author": "Raphael Jorge",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "nodemon": "2.0.2"
  },
  "dependencies": {
    "@babel/runtime": "7.9.2",
    "apollo-server-express": "^2.11.0",
    "bcrypt": "4.0.1",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "graphql": "14.6.0",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.10.0",
    "multer": "1.4.2",
    "mysql2": "2.1.0",
    "sequelize": "5.21.5",
    "sequelize-cli": "5.5.1"
  },
  "bugs": {
    "url": "https://github.com/raphaeljorge/node-graphql-auth-starter/issues"
  },
  "homepage": "https://github.com/raphaeljorge/node-graphql-auth-starter#readme"
}
